{
  "name": "xroute",
  "version": "15.7.0",
  "description": "Mobx state based router",
  "files": [
    "x/*"
  ],
  "main": "./x/cjs/index.js",
  "module": "./x/esm/index.js",
  "types": "./x/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./x/esm/index.js",
      "require": "./x/cjs/index.js"
    },
    "./schema": {
      "import": "./x/esm/XRouteSchema.js",
      "require": "./x/cjs/XRouteSchema.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfour/xroute"
  },
  "scripts": {
    "build": "pnpm build:clean; pnpm build:cjs; pnpm build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs:watch": "tsc -p tsconfig.cjs.json --watch",
    "build:esm:watch": "tsc -p tsconfig.esm.json --watch",
    "build:clean": "rm -rf x/*",
    "build:watch": "pnpm build:clean; pnpm run '/(cjs|esm):watch$/'",
    "dev": "ladle serve --stories stories/**/*.stories.tsx"
  },
  "peerDependencies": {
    "history": "5",
    "zod": ">= 3",
    "mobx": "^6"
  },
  "dependencies": {
    "@types/qs": "^6.9.15",
    "lodash": "^4.17.21",
    "microdiff": "^1.4.0",
    "path-to-regexp": "^6.2.2",
    "qs": "^6.12.1",
    "zod": "^3.23.8",
    "type-fest": "^4.18.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@ladle/react": "^4.0.3",
    "@types/lodash": "^4.17.4",
    "@types/node": "^18.11.17",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-nfour": "^3.5.0",
    "history": "^5.3.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.4.5",
    "vite": "^5.2.11"
  }
}
