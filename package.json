{
  "name": "xroute",
  "version": "3.2.1-0",
  "description": "Mobx state based router",
  "files": [
    "dist/*"
  ],
  "main": "./dist/xroute.js",
  "module": "./dist/es/xroute.js",
  "types": "./dist/es/xroute.d.ts",
  "scripts": {
    "dist": "yarn dist:cjs && yarn dist:esm",
    "dist:cjs": "tsc -p tsconfig.cjs.json",
