{
  "name": "xroute",
  "version": "4.0.0",
  "description": "Mobx state based router",
  "files": [
    "dist/*"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "dist": "yarn dist:cjs && yarn dist:esm",
    "dist:cjs": "tsc -p tsconfig.cjs.json",
    "dist:esm": "tsc -p tsconfig.esm.json",
    "dist:watch": "yarn -s dist --watch",
    "dev": "start-storybook"
  },
  "peerDependencies": {
    "history": ">=5"
  },
  "optionalDependencies": {
    "mobx": ">=6"
  },
  "dependencies": {
    "lodash-es": "~4.17.21",
    "path-to-regexp": "^6.2.0",
    "ts-toolbelt": "^8.0.7"
  },
  "devDependencies": {
    "@storybook/react": "^6.1.10",
    "@types/lodash-es": "~4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint-config-nfour": "^1.3.0",
    "history": "^5.0.0",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "~4.2.2"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "dist",
      "node_modules"
    ],
    "extends": [
      "nfour"
    ]
  },
  "prettier": "eslint-config-standard-typescript-prettier/prettier"
}
