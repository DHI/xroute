    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint-config-nfour": "^3.5.0",
    "history": "^5.3.0",
    "mobx": "^6.1.8",
